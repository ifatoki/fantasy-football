{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "consistent-return": 0,
        "comma-dangle": 0,
        "no-shadow": ["error", { "allow": ["req", "res", "err"] }]
    }
}
